{
  "name": "file-linter",
  "version": "0.0.3",
  "description": "Regex File Linter",
  "main": "./dist/file-linter",
  "bin": {
    "file-lint": "./dist/file-linter"
  },
  "scripts": {
    "prepare": "npm run dist",
    "build": "tsc",
    "dist": "npm run build && uglifyjs ./build/index.js -o ./dist/file-linter -c -m",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./build/index.js",
    "test": "jest"
  },
  "author": "Bryce R. McWilliams <https://brycemcwilliams.com>",
  "license": "MIT",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "chalk": "^2.4.2",
    "find-up": "^4.0.0",
    "globby": "^9.2.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/find-up": "^2.1.1",
    "@types/globby": "^9.1.0",
    "@types/node": "^12.0.2",
    "@types/yargs": "^13.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5",
    "uglify-js": "^3.6.0"
  }
}
